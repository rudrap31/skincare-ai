      "eas": {
        "projectId": "f289c539-164d-4d1c-9d0d-df676563f5f3"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you scan you face."
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "The app accesses your camera to let you scan you face and product barcodes."
        }
      ],
      [
        "react-native-share",
        {
