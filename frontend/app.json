{
  "expo": {
    "name": "simplyskin",
    "slug": "simplyskin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/lotusicon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/lotusicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to scan your skin.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photos to upload images.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save photos to your library.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.anonymous.simplyskin"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.anonymous.simplyskin"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "EXPO_PUBLIC_SUPABASE_URL": "https://nedoihitthkzpspfbcul.supabase.co",
